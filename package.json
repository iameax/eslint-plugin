    "eslint-module-utils": "^2.6.0",
    "mocha": "^8.3.2"
  },
  "description": "",
  "bugs": {
    "url": "https://github.com/iameax/eslint-plugin-code-style/issues"
  },
  "homepage": "https://github.com/iameax/eslint-plugin-code-style#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "eslint": "^7.24.0",
    "rimraf": "^3.0.2"
  },
  "author": "goo.eax@gmail.com",
  "license": "MIT"
