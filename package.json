{
  "name": "@iameax/eslint-plugin-code-style",
  "version": "0.0.4",
  "main": "lib/index.js",
  "files": [
    "*.md",
    "LICENSE",
    "docs",
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel --quiet --out-dir lib src",
    "test": "mocha --recursive \"./tests/**/*.js\"",
    "bump-version": "yarn version --patch",
    "postversion": "git push --follow-tags && yarn publish . --access public",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iameax/eslint-plugin-code-style.git"
  },
